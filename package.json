    "boxen": "^8.0.1",
    "chalk": "^4.1.2",
    "cli-progress": "^3.12.0",
    "commander": "^13.1.0",
    "inquirer": "^8.2.6",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.3"
  },
  "devDependencies": {
    "@types/chalk": "^2.2.0",
    "@types/inquirer": "^8.2.10",
    "rimraf": "^6.0.1"
  }
